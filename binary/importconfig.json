      {
         "name":"PIB RES IMPORT Decl D",
         "active": true,
         "processId": false,
         "source":{
            "table":"TblPIBResNPD",
            "query":"select * from (select  a.CAR as SUBMISSION_CAR, DateSerial(val(MID(a.CAR, 13, 4)), val(MID(a.CAR, 17, 2)), val(MID(a.CAR, 19, 2))) as SUBMISSION_DT, MID(a.CAR, 21, 6) as SUBMISSION_NO, a.SERIAL, a.NOHS as HS_NO, a.SERITRP as TARIFF_SERIAL, a.BRGURAI as DESC_OF_GOODS, a.MERK, a.TIPE, a.BRGASAL as COUNTRY_CD, (SELECT UREDI FROM tblNegara x where x.KdEdi = a.BRGASAL) as COUNTRY_NAME, a.DNILINV as PART_FOB, a.DCIF as PART_CIF, a.KDSAT as MEASUREMENT_CD, (SELECT UREDI FROM tblSatuan x where x.KdEdi = a.KDSAT) as MEASUREMENT_DESC, a.JMLSAT as PART_QTY, (select x.TRPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '0') as ID_TAX_NON_EXEMP_TARIFF, (select x.TRPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and  y.KDFASBM = '1') as ID_TAX_SUSPENDED_TARIFF, (select x.TRPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '2') as ID_TAX_RELEASED_TARIFF, (select x.TRPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and  y.KDFASBM = '3') as ID_TAX_PERIODICAL_TARIFF, (select x.TRPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and  y.KDFASBM = '4') as ID_TAX_GOV_BORNED_TARIFF, (select SUM(x.TRPBM)* z.ndpbm * a.DCIF * y.FASBM / 100 from tblPibTrf x, tblPibFas y, tblPibHdr z where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and a.CAR = z.CAR group by z.ndpbm , a.DCIF,y.FASBM) as ID_TAX_AMT, (select x.FASBM from tblPIbFas x where x.CAR = a.CAR and x.SERIAL= a.SERIAL) as ID_TAX_AD_VALOREM,  (select x.TRPPPN from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '0') as VA_TAX_NON_EXEMP_TARIFF, (select x.TRPPPN from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '1') as VA_TAX_SUSPENDED_TARIFF, (select x.TRPPPN from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '2') as VA_TAX_RELEASED_TARIFF, (select x.TRPPPN from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '3') as VA_TAX_PERIODICAL_TARIFF, (select x.TRPPPN from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '4') as VA_TAX_GOV_BORNED_TARIFF, (select SUM(x.TRPPPN)* z.ndpbm * a.DCIF * y.FASPPN / 100 from tblPibTrf x, tblPibFas y, tblPibHdr z where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and a.CAR = z.CAR group by z.ndpbm , a.DCIF,y.FASPPN) as VA_TAX_AMT, (select x.FASPPN from tblPIbFas x where x.CAR = a.CAR and x.SERIAL= a.SERIAL) as VA_TAX_AD_VALOREM,  (select x.TRPPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '0') as LUX_TAX_NON_EXEMP_TARIFF, (select x.TRPPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '1') as LUX_TAX_SUSPENDED_TARIFF, (select x.TRPPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '2') as LUX_TAX_RELEASED_TARIFF, (select x.TRPPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '3') as LUX_TAX_PERIODICAL_TARIFF, (select x.TRPPBM from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '4') as LUX_TAX_GOV_BORNED_TARIFF, (select SUM(x.TRPPBM)* z.ndpbm * a.DCIF * y.FASPBM / 100 from tblPibTrf x, tblPibFas y, tblPibHdr z where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and a.CAR = z.CAR group by z.ndpbm , a.DCIF,y.FASPBM) as LUX_TAX_AMT, (select x.FASPBM from tblPIbFas x where x.CAR = a.CAR and x.SERIAL= a.SERIAL) as LUX_TAX_AD_VALOREM,  (select x.TRPPPH from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '0') as INC_TAX_NON_EXEMP_TARIFF, (select x.TRPPPH from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '1') as INC_TAX_SUSPENDED_TARIFF, (select x.TRPPPH from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '2') as INC_TAX_RELEASED_TARIFF, (select x.TRPPPH from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '3') as INC_TAX_PERIODICAL_TARIFF, (select x.TRPPPH from tblPibTrf x, tblPibFas y where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and y.KDFASBM = '4') as INC_TAX_GOV_BORNED_TARIFF, (select SUM(x.TRPPPH)* z.ndpbm * a.DCIF * y.FASPPH / 100 from tblPibTrf x, tblPibFas y, tblPibHdr z where x.CAR = a.CAR and  x.NOHS = a.NOHS and x.SERITRP = a.SERITRP and y.CAR = a.CAR and y.SERIAL = a.SERIAL and a.CAR = z.CAR group by z.ndpbm , a.DCIF,y.FASPPH) as INC_TAX_AMT, (select x.FASPPH from tblPIbFas x where x.CAR = a.CAR and x.SERIAL= a.SERIAL) as INC_TAX_AD_VALOREM    from tblPibDtl a    inner join tblPibRes b on a.CAR = b.CAR    WHERE DateSerial(val(MID(a.CAR, 13, 4)),val(MID(a.CAR, 17, 2)), val(MID(a.CAR, 19, 2))) BETWEEN Date()-50 AND Date() and b.RESKD = '300') x          "
         },
